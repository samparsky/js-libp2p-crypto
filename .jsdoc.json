{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/"],
        "exclude": ["node_modules"]
    },
    "opts": {
        "destination": "src/",
        "template": "./node_modules/tsd-jsdoc/dist",
        "recurse": true
    },
    "plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"]
}